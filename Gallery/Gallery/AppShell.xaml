<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Gallery.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Gallery"
    Shell.FlyoutBehavior="Flyout"
    Title="Gallery">
    
    
    
    <TabBar>
        <Tab Title="Library" Icon="library.png">

            <ShellContent ContentTemplate ="{DataTemplate local:library}" />
        </Tab>
        <Tab Title="Albums" Icon="home.png" >

            <ShellContent ContentTemplate ="{DataTemplate local:Album}" />
            
        </Tab> 
        <Tab Title="Camera" Icon="camera.png" >

            <ShellContent ContentTemplate ="{DataTemplate local:Camera}" />
        </Tab>

    </TabBar>

    

</Shell>
